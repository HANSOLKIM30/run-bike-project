<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.teamrun.runbike.stamp.dao.StampDao">

	<!-- Result Mapping 설정 -->
	<resultMap id="stamp"
		type="com.teamrun.runbike.stamp.domain.Stamp">
		<id property="s_num" column="s_num" />
		<result property="s_name" column="s_name" />
		<result property="s_photo" column="s_photo" />
		<result property="cs_photo" column="cs_photo" />
		<result property="s_point_lon" column="s_point_lon" />
		<result property="s_point_lat" column="s_point_lat" />
	</resultMap>
	
<!-- create -->
<insert id="insertStmap" parameterType="com.teamrun.runbike.stamp.domain.Stamp">
	insert into stamp (s_name, s_photo, cs_photo, s_point_lon, s_point_lat) values (#{s_name}, #{s_photo}, #{cs_photo}, #{s_point_lon}, #{s_point_lat})	
</insert>

<!-- select -->
<select id="selectMyStamp" resultMap="stamp">
		select * from stamp
</select>

</mapper>